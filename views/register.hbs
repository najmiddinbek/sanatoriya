<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.css" />
<script src="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.js"></script>
<div class="container w-75 mx-auto">
    <div class="flex">
        <svg data-aos="fade-down" data-aos-duration="1000" data-aos-delay="1000" class="home_svg" width="" height=""
            viewBox="0 0 185 69" fill="none" xmlns="http://www.w3.org/500/svg"
            xmlns:xlink="http://www.w3.org/1999/xlink">
            <rect width="185" height="69" fill="url(#pattern0)" />
            <dezfs>
                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                    <use xlink:href="#image0_23_46" transform="matrix(0.00666667 0 0 0.0178744 0 -0.000483092)" />
                </pattern>
                <image id="image0_23_46" width="150" height="56"
                    xlink:href="data:image/png;base64," />
            </dezfs>
        </svg>
        <div data-aos="fade-leftfds" data-aos-duration="2000" data-aos-delay="3000">
            <a class="add_user" href="/register" data-aos="fade-down" data-aos-duration="1000"
                data-aos-delay="1000">Shaxsni tasdiqlash</a>
        </div>
    </div>
    <h1 class="fs-1 mb-4" data-aos="fade-down" data-aos-duration="1000" data-aos-delay="1000">Shaxsni tasdiqlash</h1>
    {{#if registerError}}
    <p>{{registerError}}</p>
    {{/if}}
    <form action="/register" method="post">
        <div class="tasdiqlash">
            <h1 class="title" data-aos="fade-up" data-aos-duration="1000" data-aos-delay="1000">
                Shaxsingizni tasdiqlang
            </h1>
            <div class="d-flex gap-3">
                <div class="form-floating mb-3 w-50">
                    <input type="text" data-aos="fade-up" data-aos-duration="1000" data-aos-delay="1200"
                        class="form-control" id="floatingInput" name="ism" placeholder="Ism kiriting...">
                </div>
                <div class="form-floating mb-3 w-50">
                    <input type="text" data-aos="fade-up" data-aos-duration="1000" data-aos-delay="1400"
                        class="form-control" id="floatingInput" name="familiya" placeholder="Familiya kiriting">
                </div>
            </div>
            <div class="form-floating mb-3">
                <input type="email" data-aos="fade-up" data-aos-duration="1000" data-aos-delay="1600"
                    class="form-control" id="floatingInput" name="email" placeholder="name@example.com">
            </div>
            <div class="form-floating">
                <input type="password" data-aos="fade-up" data-aos-duration="1000" data-aos-delay="1800"
                    class="form-control" id="floatingPassword" name="password" placeholder="Password">
            </div>
            <div class="text-end" data-aos="fade-left" data-aos-duration="1000" data-aos-delay="1500">
                <a class="add_user" href="/" data-aos-delay="1500">Tasdiqlash</a>
            </div>
        </div>
    </form>
</div>


<style>
    body {
        background: linear-gradient(119deg, #D8D2FC 17.58%, #D8D2FC 17.58%, #D8D2FC 17.59%, #FDE2E0 41.89%, #E6E4EA 68%, #E0E9F4 103.34%, rgba(230, 225, 232, 0.00) 103.35%, #E0E9F4 103.35%);
    }

    .container {
        max-width: 1200px;
        width: 100%;
        margin: 0 auto;
    }

    .flex {
        display: flex;
        align-items: center;
        justify-content: space-between;
    }

    .flex-2 {
        display: flex;
    }

    form {
        background: white;
        display: flex;
        justify-content: center;
        border-radius: 47px;
        padding-bottom: 33px;
    }

    .form-floating {
        margin-bottom: 20px;
    }

    .form-floating input {
        width: 1000px;
        padding: 15px 20px;
    }

    .form-floating input {
        padding: 10px 4px;
        margin-bottom: 10px;
        border-top: none;
        border-right: none;
        border-left: none;
        stroke-width: 1px;
        stroke: rgba(39, 30, 74, 0.50);
        opacity: 0.6;
        background: transparent;
        outline: none;
        opacity: .7;
        transition: .1s;
        color: #271E4A;
        font-family: "Poppins", sans-serif;
        font-size: 18px;
        font-style: normal;
        font-weight: 500;
    }

    .form-floating input:focus {
        opacity: 1;
        color: black;
    }

    .text-end {
        text-align: end;
    }

    .add_user {
        border-radius: 5px;
        padding: 10px 25px;
        border: none;
        background: transparent;
        background: #271E4A;
        color: #fff;
        font-family: "Poppins", sans-serif;
        cursor: pointer;
        border: 2px solid transparent;
        transition: .4s ease-in-out;
        text-decoration: none;
    }

    .add_user:hover {
        color: #271E4A;
        background: transparent;
        border: 2px solid #271E4A;
    }

    @media(min-width: 0px) and (max-width: 500px) {
        .title {
            font-size: 18px;
        }

        form {
            display: block;
            padding: 10px;
        }
    }

    @media(min-width: 0px) and (max-width: 1500px) {
        .form-floating input {
            width: 100%;
        }

        form {
            display: block;
            padding: 10px 20px;
        }
    }
</style>


<script>
    AOS.init();
</script>