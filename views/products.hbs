<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.css" />
<script src="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.js"></script>

<div class="flex">
    <svg data-aos="fade-down" data-aos-duration="1000" data-aos-delay="1000" width="185" height="69"
        viewBox="0 0 185 69" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <rect width="185" height="69" fill="url(#pattern0)" />
        <defs>
            <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                <use xlink:href="#image0_23_46" transform="matrix(0.00666667 0 0 0.0178744 0 -0.000483092)" />
            </pattern>
            <image id="image0_23_46" width="150" height="56"
                xlink:href="data:image/png;base64," />
        </defs>
    </svg>
    <div data-aos="fade-left" data-aos-duration="1000" data-aos-delay="1500">
        <a class="add_user" href="/register" data-aos-delay="1500">Shaxsni tasdiqlash</a>
    </div>
</div>

<h1 data-aos="fade-left" data-aos-duration="1000" data-aos-delay="1500">Barcha Be'morlar</h1>

{{#if barchabemorlar.length}}
<div class="all_users" data-aos-duration="1000" data-aos-delay="1500">
    <h1 data-aos="fade-down" data-aos-duration="1000" data-aos-delay="1500">Barcha bemorlar ma'lumotlari</h1>
    <table data-aos="fade-up" data-aos-duration="1000" data-aos-delay="1500">
        <thead>
            <tr>
                <th>Ismi</th>
                <th>Familiyasi</th>
                <th>Telefon Raqami</th>
                <th>Jinsi</th>
                <th>Yoshi</th>
                <th>Yashash manzili</th>
                <th>Tahrirlash</th>
            </tr>
        </thead>
        {{#each barchabemorlar}}
        <tbody>
            <tr>
                <th>{{yangiFoydalanuvchiIsmi}}</th>
                <th>{{yangiFoydalanuvchiFamilyasi}}</th>
                <th>{{yangiFoydalanuvchitelefonRaqami}}</th>
                <th>{{selectOptionJins}}</th>
                <th>{{yangifoydalanuvchiYoshi}}</th>
                <th>{{yashashManzili}}</th>
                <th>
                    <div class="main_buttons">
                        <form class="delete_button" action="/delete-product/{{_id}}" method="post">
                            <button type="submit" class=""><i class="bi bi-trash"></i></button>
                        </form>
                        <a type="button" class="main_button bi bi-pencil-square" href="/edit-bemor/{{_id}}"></a>
                        <a type="button" class="main_button bi bi-eye" href="/see-bemor/{{_id}}"></a>
                    </div>
                </th>
            </tr>
        </tbody>
        {{/each}}
    </table>
</div>
{{else}}
<div class="no_users_img">
    <img src="https://www.taxmann.com/post/wp-content/uploads/2021/06/1071-scaled.jpg" alt="">
    <div class="">
        <h1>Hozircha be'morlar yo`q</h1> <br>
        <a href="/add" class="button">Be'mor qo`shish</a>
    </div>
</div>
{{/if}}

<style>
    body {
        background: linear-gradient(119deg, #D8D2FC 17.58%, #D8D2FC 17.58%, #D8D2FC 17.59%, #FDE2E0 41.89%, #E6E4EA 68%, #E0E9F4 103.34%, rgba(230, 225, 232, 0.00) 103.35%, #E0E9F4 103.35%);
    }

    table {
        border-collapse: collapse;
    }

    .container {
        max-width: 1200px;
        width: 100%;
        margin: 0 auto;
    }

    .flex {
        display: flex;
        align-items: center;
        justify-content: space-between;
    }

    .flex-2 {
        display: flex;
        gap: 20px;
    }

    .text,
    .paragraf {
        margin: 0;
    }

    thead {
        background: #EDF4FC;
    }

    .no_users_img {
        display: grid;
        grid-template-columns: auto;
        justify-content: center;
        text-align: center;
        gap: 40px;
    }

    .no_users_img img {
        width: 500px;
    }

    .button {
        border-radius: 5px;
        padding: 15px 35px;
        border: none;
        background: transparent;
        background: #271E4A;
        color: #fff;
        font-family: "Poppins", sans-serif;
        cursor: pointer;
        border: 2px solid transparent;
        transition: .4s ease-in-out;
        text-decoration: none;
    }

    .all_users {
        padding: 36px 33px;
        border-radius: 47px;
        background: #FFF;
        box-shadow: 0px 4px 15px 0px rgba(216, 210, 252, 0.64);
    }

    thead tr th {
        padding: 10px 50px;
    }

    tbody tr th {
        border-right: 1px solid #E0E9F4;
    }

    tbody tr th {
        border-bottom: 1px solid #E0E9F4;
    }

    .delete_button button {
        background: transparent;
        border: none;
        border-radius: 8px;
        background: #FFF;
        box-shadow: 0px 4px 15px 0px rgba(185, 178, 226, 0.64);
        padding: 10px;
        font-size: 18px;
        cursor: pointer;
    }

    .main_button {
        border: none;
        border-radius: 8px;
        background: #FFF;
        box-shadow: 0px 4px 15px 0px rgba(185, 178, 226, 0.64);
        font-size: 18px;
        cursor: pointer;
        text-decoration: none;
        color: black;
        padding: 0 10px;
        height: 40px;
        display: flex;
        align-items: center;
    }

    .main_buttons {
        display: flex;
        gap: 10px;
        justify-content: center;
        color: black;
    }

    .add_user {
        border-radius: 5px;
        padding: 15px 35px;
        border: none;
        background: transparent;
        background: #271E4A;
        color: #fff;
        font-family: "Poppins", sans-serif;
        cursor: pointer;
        border: 2px solid transparent;
        transition: .4s ease-in-out;
        text-decoration: none;
    }

    .buttons .add_user:hover {
        color: #271E4A;
        background: transparent;
        border: 2px solid #271E4A;
    }
</style>


<script>
    AOS.init();
</script>