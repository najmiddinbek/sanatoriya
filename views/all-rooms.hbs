<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.css" />
<script src="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.js"></script>

<div class="flex">
    <svg data-aos="fade-down" data-aos-duration="1000" data-aos-delay="1000" width="185" height="69"
        viewBox="0 0 185 69" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <rect width="185" height="69" fill="url(#pattern0)" />
        <defs>
            <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                <use xlink:href="#image0_23_46" transform="matrix(0.00666667 0 0 0.0178744 0 -0.000483092)" />
            </pattern>
            <image id="image0_23_46" width="150" height="56"
                xlink:href="data:image/png;base64," />
        </defs>
    </svg>
    <div data-aos="fade-left" data-aos-duration="1000" data-aos-delay="1500">
        <a class="add_user" href="/register" data-aos-delay="1500">Shaxsni tasdiqlash</a>
    </div>
</div>

<h1 data-aos="fade-left" data-aos-duration="1000" data-aos-delay="1500">Barcha Xonalar</h1>
<div class="add_room">
    <a href="/add-room" class="add_user">Xona qo`shish</a>
</div>

{{#if barchaxonalar.length}}
<div class="all_users" data-aos-duration="1000" data-aos-delay="1500">
    <table data-aos="fade-up" data-aos-duration="1000" data-aos-delay="1500">
        {{#each barchaxonalar}}
        <div class="addest_users">
            <div data-aos="fade-right" data-aos-duration="1000" data-aos-delay="1500" class="card">
                <img src="https://ww1.prweb.com/prfiles/2017/02/28/14108075/WesleyChapelpatientroom.jpg" alt="">
                <div class="padding_texts">
                    <p>Xona narxi: {{selectOptionCost}}</p>
                    <p>Xona turi: {{selectOptionType}}</p>
                    <p>Xona "{{selectOptionDay}}" kundan kegin ochiladi</>
                        <a href="{{yangiXonaVideosi}}"></a>
                    <div class="main_buttons">
                        <form class="delete_button" action="/delete-room/{{_id}}" method="post">
                            <button type="submit" class="delet_button">
                                <i class="bi bi-trash"></i>
                            </button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </table>
</div>
{{else}}
<div class="no_users_img">
    <img src="https://www.taxmann.com/post/wp-content/uploads/2021/06/1071-scaled.jpg" alt="">
    <div class="flex-2">
        <h1>Hozircha xonalar yo`q</h1>
        <a href="/add">Xona qo`shish</a>
    </div>
</div>
{{/if}}

<style>
    body {
        background: linear-gradient(119deg, #D8D2FC 17.58%, #D8D2FC 17.58%, #D8D2FC 17.59%, #FDE2E0 41.89%, #E6E4EA 68%, #E0E9F4 103.34%, rgba(230, 225, 232, 0.00) 103.35%, #E0E9F4 103.35%);
    }

    .container {
        max-width: 1200px;
        width: 100%;
        margin: 0 auto;
    }

    .flex {
        display: flex;
        align-items: center;
        justify-content: space-between;
    }

    .no_users_img {
        display: grid;
        grid-template-columns: auto;
        justify-content: center;
        text-align: center;
        gap: 40px;
    }

    .no_users_img img {
        width: 500px;
    }

    .all_users {
        display: grid;
        grid-template-columns: auto auto;
        gap: 30px;
        background: white;
        justify-content: center;
        padding: 10px;
        margin-top: 50px;
        border-radius: 10px;
    }

    .all_users .card {
        border: 1px solid #9999992f;
        border-radius: 8px;
        background: rgba(255, 255, 255, 0.10);
        backdrop-filter: blur(35px);
        position: relative;
    }

    .card:hover .main_buttons {
        opacity: 1;
    }


    .main_buttons {
        position: absolute;
        top: -20px;
        transition: .3s;
        opacity: 0;
        right: 0;
    }

    img {
        width: 100%;
        border-radius: 8px;
    }

    .add_user {
        border-radius: 5px;
        padding: 8px 25px;
        border: none;
        background: transparent;
        background: #271E4A;
        color: #fff;
        font-family: "Poppins", sans-serif;
        cursor: pointer;
        border: 2px solid transparent;
        transition: .4s ease-in-out;
        text-decoration: none;
    }

    .padding_texts {
        padding: 0px 30px;
    }

    p {
        font-size: 25px;
        font-weight: 700;
        color: #271E4A;
    }

    .add_room {
        text-align: end;
    }

    .delet_button {
        background: white;
        width: 50px;
        height: 50px;
        border: none;
        font-size: 20px;
        cursor: pointer;
    }
</style>


<script>
    AOS.init();
</script>